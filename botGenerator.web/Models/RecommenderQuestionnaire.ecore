    <eStructuralFeatures xsi:type="ecore:EReference" name="unknownOptions" upperBound="-1"
        eType="#//Option" eOpposite="#//Option/unknown"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="unavailableOptions" upperBound="-1"
