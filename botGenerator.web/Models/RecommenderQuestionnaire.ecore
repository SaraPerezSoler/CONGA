    <eStructuralFeatures xsi:type="ecore:EReference" name="unknown" upperBound="-1"
        eType="#//Tool" eOpposite="#//Tool/unknownOptions"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="possible" upperBound="-1"
        eType="#//Tool" eOpposite="#//Tool/possibleOptions"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="WithName" abstract="true">
